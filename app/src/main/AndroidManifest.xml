<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maddy.miwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/miwok"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/miwok"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Colors"
            android:label="Colors" android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Phrases"
            android:label="Phrases" android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".Family"
            android:label="Family" android:parentActivityName=".MainActivity"/>
        <activity android:name=".Numbers" android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>